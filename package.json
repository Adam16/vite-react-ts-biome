{
	"name": "vite-react-ts-biome",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "biome check .",
		"lint:write": "biome check --write .",
		"preview": "vite preview",
		"prepare": "husky",
		"test": "vitest"
	},
	"dependencies": {
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.1",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@vitejs/plugin-react": "^4.6.0",
		"globals": "^16.3.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.2",
		"typescript": "~5.8.3",
		"vite": "^7.0.4",
		"vitest": "^3.2.4"
	},
	"lint-staged": {
		"*": [
			"biome check --write --no-errors-on-unmatched --files-ignore-unknown=true"
		]
	},
	"engines": {
		"node": ">=22.0.0"
	},
	"packageManager": "pnpm@10.11.0"
}
